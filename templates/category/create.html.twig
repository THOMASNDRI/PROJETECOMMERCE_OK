{% extends "base.html.twig" %}

{% block title %}
	Création d'une categorie
{% endblock %}

{% block content %}
	<h1>Nouvelle categorie</h1>
	{{ form_start(formView) }}

	{{ form_errors(formView) }}

	<div class="col">
		<div class="row">
			{{ form_row(formView.name) }}
		</div>
	</div>
	<button type="submit" class="btn btn-primary">
		<i class=" fas fa-save"></i>
		Créer la category
	</button>

	{{ form_end(formView) }}
{% endblock %}
